      "source": [
        "import shutil\n",
        "import os\n",
        "\n",
        "def folder_to_zip(folder_path, zip_name, output_path):\n",
        "    # Check if the folder exists\n",
        "    if not os.path.isdir(folder_path):\n",
        "        print(\"The folder does not exist.\")\n",
        "        return\n",
        "\n",
        "    # Create a zip file from the folder and specify the output path\n",
        "    shutil.make_archive(output_path, 'zip', folder_path)\n",
        "    print(f\"Folder '{folder_path}' has been successfully zipped to '{output_path}.zip'.\")\n",
        "\n",
        "# Example usage\n",
        "folder_path = '/content/drive/MyDrive/Final_folder_code_thesis/features_WildRF'\n",
        "zip_name = 'features_WildRF'  # Name for the zip file (without extension)\n",
        "output_path = '/content/drive/MyDrive/Final_folder_code_thesis/features_WildRF'  # Output path in Google Drive\n",
        "\n",
        "folder_to_zip(folder_path, zip_name, output_path)"
